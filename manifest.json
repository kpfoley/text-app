{
  "name": "Distraction-Free Text Editor",
  "short_name": "DFTE",
  "description": "A fork of the native Chrome OS text editor with deliberately limited functionality for classroom use.",
  "default_locale": "en",
  "version": "0.5.0.8",
  "icons": {
    "16": "icon/16x16.png",
    "32": "icon/32x32.png",
    "48": "icon/48x48.png",
    "64": "icon/64x64.png",
    "96": "icon/96x96.png",
    "128": "icon/128x128.png",
    "256": "icon/256x256.png"
  },
     "key": "",
  "manifest_version": 2,
  "minimum_chrome_version": "31.0",
  "offline_enabled": true,
  "kiosk_enabled": true,
 "app": {
    "background": {
      "scripts": [
        "js/background.js",
        "js/util.js"
      ]
    }
  },
  "permissions": [
    "app.window.alwaysOnTop",
    {"fileSystem": ["write", "retainEntries"]},
    "storage",
    "https://www.google-analytics.com/",
    "identity"
  ],
  "file_handlers": {
    "text": {
      "types": ["text/*"],
      "extensions": ["text", "txt"]
    }
  }
}
